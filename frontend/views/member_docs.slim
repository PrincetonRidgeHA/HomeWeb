    == slim :_partial_dashboard_navigation
    <div class="container-fluid">
      <div class="row">
        <div class="col-sm-12">
          <h1 class="page-header">Documents</h1>
          <div class="table-responsive">
            <table class="table table-striped">
              <thead>
                <tr>
                  <th>Document Name</th>
                  <th>Upload Date</th>
                  <th>Uploaded By</th>
                  <th>Actions</th>
                </tr>
              </thead>
              <tbody>
                - @items.each do |item|
                  <tr>
                    <td>
                      =item.name
                    </td>
                    <td>
                      =item.uploaddate
                    </td>
                    <td>
                      =item.uploadedby
                    </td>
                    <td>
                      <a href="#{item.url}">Download >></a>
                    </td>
                  </tr>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
